# Add user info later on local for specific account
[user]
    email = hvngcs@gmail.com
    name = Hung Nguyen
[core]
    editor = vim
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = green
    changed = yellow
    untracked = red
[http]
    sslVerify = false
[alias]
    mine = !git config user.name 'Hung Nguyen' && git config user.email 'hvngcs@gmail.com'
